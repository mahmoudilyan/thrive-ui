{
  "name": "@thrive/ui",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css",
    "./tokens.css": {
      "development": "./src/styles/tokens.css",
      "default": "./src/styles/tokens.css"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -f dist/*.js dist/*.mjs && pnpm tailwindcss -i src/styles/globals.css -o dist/style.css --minify && tsup",
    "build:css": "tailwindcss -i src/styles/globals.css -o dist/style.css --minify",
    "tailwind": "tailwindcss -i src/styles/globals.css -o dist/style.css --minify",
    "dev": "tsup --watch",
    "dev:js": "tsup --watch",
    "dev:css": "tailwindcss -i src/styles/globals.css -o dist/style.css --watch",
    "prepublishOnly": "pnpm build",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@diceui/tags-input": "^0.7.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.12.2",
    "lucide-react": "^0.469.0",
    "radix-ui": "^1.4.3",
    "react-day-picker": "^9.6.7",
    "react-hook-form": "^7.63.0",
    "react-icons": "^5.4.0",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
